import java.util.*;
class FibonicieSeries
{
  	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the number...");
		int k = s.nextInt();
		int a=0,b=1,c=1;
		for(int i = 1; i <=k; i++)
		{
        a = b;
        b = c;
		c = a+b;
		System.out.print(a+" ,");
		}
	}
}
